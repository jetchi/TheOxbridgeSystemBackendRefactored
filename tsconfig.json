{
    "compilerOptions": {
        "module": "commonjs",
        "skipLibCheck": true,
        "esModuleInterop": true,
        "target": "es6",
        "noImplicitAny": true, // false for multer stuff?
        "moduleResolution": "node",
        "sourceMap": true,
        "outDir": "./dist",
        "baseUrl": ".",
        "paths": {
            "*": [
                "node_modules/*"
            ]
        }
    },
    "include": [
        "src/**/*"
    ]
    
}